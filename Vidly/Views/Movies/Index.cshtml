@model List<Movie>

<h1>Movies</h1>

@if (Model == null || Model.Count == 0)
{
    <p>We don't have any movies yet.</p>
}
else
{
    <table class="table table-hover">
        <thead>
            <tr>
                <th scope="col">Customers</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var movie in Model)
            {
                <tr>
                    <td>
                        @movie.Name
                    </td>
                </tr>

            }
        </tbody>
    </table>
}
